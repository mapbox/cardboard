{
  "name": "@mapbox/cardboard",
  "version": "2.2.3-dev1",
  "description": "A library for storing and searching geographic features",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint index.js lib test/*.js",
    "test": "nyc tape test/*.test.js",
    "coverage": "nyc report --reporter html && opener coverage/index.html",
    "docs": "documentation build index.js -f md --github > api.md",
    "posttest": "rm -r ./test/test"
  },
  "bin": {
    "cardboard": "./bin/cardboard.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mapbox/cardboard.git"
  },
  "keywords": [
    "geographic",
    "index",
    "spatial"
  ],
  "author": "Mapbox",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mapbox/cardboard/issues"
  },
  "homepage": "https://github.com/mapbox/cardboard",
  "dependencies": {
    "@mapbox/dyno": "1.4.1",
    "aws-sdk": "^2.5.3",
    "cuid": "1.2.4",
    "geobuf": "0.2.5",
    "geojson-extent": "^0.1.0",
    "geojson-normalize": "0.0.0",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "queue-async": "~1.0.7",
    "sphericalmercator": "^1.0.3",
    "through2": "^2.0.0",
    "tilebelt": "^0.5.2"
  },
  "devDependencies": {
    "@mapbox/dynamodb-test": "^0.5.0",
    "documentation": "^13.0.2",
    "dynalite": "^1.3.2",
    "eslint": "^7.9.0",
    "geojson-fixtures": "0.1.0",
    "geojson-random": "^0.2.2",
    "geojson-stream": "0.0.0",
    "mock-aws-s3": "^0.2.1",
    "nyc": "^15.1.0",
    "opener": "^1.5.2",
    "tape": "^4.0.0"
  }
}
